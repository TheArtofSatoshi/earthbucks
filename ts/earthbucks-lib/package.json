{
  "name": "earthbucks-lib",
  "version": "0.1.7",
  "description": "EarthBucks library for data structures and algorithms.",
  "main": "dist/lib.js",
  "types": "dist/lib.d.js",
  "scripts": {
    "clean": "rimraf dist",
    "main": "tsx src/main.ts",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "engines": {
    "node": ">=20.8.0",
    "pnpm": ">=6.0.0"
  },
  "keywords": [],
  "author": "Ryan X. Charles <ryan@ryanxcharles.com> (http://ryanxcharles.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/earthbucks/earthbucks"
  },
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@noble/hashes": "^1.4.0",
    "@types/secp256k1": "^4.0.6",
    "blake3": "2.1.7",
    "blake3-wasm": "2.1.5",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "earthbucks-opt-res": "^0.1.5",
    "mathjs": "^12.4.1",
    "rimraf": "^5.0.7",
    "secp256k1": "^5.0.0",
    "tsx": "^4.7.1"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^9.2.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript-eslint": "^7.9.0",
    "vitest": "^1.6.0"
  }
}
