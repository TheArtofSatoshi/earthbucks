{
  "name": "earthbucks-opt-res",
  "version": "0.1.2",
  "description": "Rust's Option and Result enums for TypeScript.",
  "main": "dist/lib.js",
  "types": "dist/lib.d.js",
  "scripts": {
    "clean": "rimraf dist",
    "main": "tsx src/main.ts",
    "test": "jest --config jest.config.cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "engines": {
    "node": ">=20.8.0",
    "pnpm": ">=6.0.0"
  },
  "keywords": [],
  "author": "Ryan X. Charles <ryan@ryanxcharles.com> (http://ryanxcharles.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/earthbucks/earthbucks"
  },
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "rimraf": "^5.0.7",
    "tsx": "^4.7.1"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@eslint/js": "^9.2.0",
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^9.2.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  }
}
